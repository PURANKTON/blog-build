  let initFn = window.walineFn || null

  const initWaline = (Fn) => {
    const waline = Fn(Object.assign({
      el: '#waline-wrap',
      serverURL: 'communicate.zhngjah.asia',
      pageview: false,
      dark: 'html[data-theme="dark"]',
      path: window.location.pathname,
